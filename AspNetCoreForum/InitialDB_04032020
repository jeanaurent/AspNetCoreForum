CREATE PROCEDURE public.messageaddoredit(integer, date, text, text, text, text, text)
    LANGUAGE plpgsql
    AS $_$
BEGIN
	IF id = 0 THEN
		INSERT INTO message(date,category,author,subject,content,status)
		VALUES ($2,$3,$4,$5,$6,$7);
	ELSE
    	UPDATE message 
    	SET date = $2,
		category = $3,
		author = $4,
		subject = $5,
		content = $6,
		status = $7
    	WHERE id = $1;
 	END IF;
	
    COMMIT;
END;
$_$;


ALTER PROCEDURE public.messageaddoredit(integer, date, text, text, text, text, text) OWNER TO postgres;

